<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:bcont="using:HotPotPlayer2.Controls.Bilibili"
			 xmlns:vm="using:HotPotPlayer2.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:DataType="vm:BilibiliPageViewModel"
             x:Class="HotPotPlayer2.Views.Pages.Bilibili">

	<Grid x:Name="Root" Background="White">

		<bcont:Header
			VerticalAlignment="Top"
			SelectedIndex="{Binding SelectedSubPage, Mode=TwoWay}"
			OnRefreshClick="RefreshClick"/>
	</Grid>
</UserControl>
