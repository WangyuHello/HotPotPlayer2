<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="48" d:DesignHeight="450"
             x:Class="HotPotPlayer2.Controls.MainSidebar">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="60" />
			<RowDefinition Height="*" />
			<RowDefinition Height="Auto" />
			<RowDefinition Height="Auto" />
			<RowDefinition Height="Auto" />
			<RowDefinition Height="Auto" />
			<RowDefinition Height="*" />
			<RowDefinition Height="60" />
		</Grid.RowDefinitions>

		<Button>
			<Label FontFamily="{DynamicResource SegoeIcons}" Content="&#xE64E;"/>
		</Button>
		<Button Grid.Row="2" Margin="0,4" >
			<Label FontFamily="{DynamicResource SegoeIcons}" Content="&#xE93C;"/>
		</Button>
		<Button Grid.Row="3" Margin="0,4" >
			<Label FontFamily="{DynamicResource SegoeIcons}" Content="&#xE29B;"/>
		</Button>
		<Button Grid.Row="4" Margin="0,4" >
			<Label FontFamily="{DynamicResource BiliIcons}" Content="&#xEA01;"/>
		</Button>
		<Button Grid.Row="5" Margin="0,4" >
			<Label FontFamily="{DynamicResource BiliIcons}" Content="&#xEA08;"/>
		</Button>
		<Button Grid.Row="7">
			<Label FontFamily="{DynamicResource SegoeIcons}" Content="&#xE713;"/>
		</Button>
</Grid>
</UserControl>
